//my current age 
const myAge = 27;
//early years
let earlyYears = 2;
earlyYears *= 10.5;
// later years
let laterYears = myAge - 2;
laterYears *= 4;
console.log(earlyYears);
console.log(laterYears);
let myAgeInDogYears = earlyYears + laterYears;
let myName = "Christina". toLowerCase();
console.log(`My name is ${myName}. I am ${myAgeInDogYears} AGE years old in dog years.`);
